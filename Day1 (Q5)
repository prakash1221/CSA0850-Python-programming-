n=float(input("enter the number of fresh loaves purchased"))
m=float(input("enter the number of day old loaves purchased"))
a=n*185
c=m*(185*60/100)
b=(m*185)-c
total_price=a+b
print("regular price=185.00")
print("amount of new loaves",a) 
print("amount of old day loaves",b)
print("total amount",total_price)
